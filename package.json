{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "serve": "nodemon --exec ts-node src/index.ts",
    "lint": "vue-cli-service lint",
    "build": "tsc -p"
  },
  "repository": {
    "url": "https://gitlab.com/vuetube/server.git",
    "type": "git"
  },
  "author": {
    "name": "Trần Đình Thiện"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "eslint": "^7.11.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.8",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.5",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --single-quote --trailing--comma all --tab-width 4 --write  'src/**/*.{js,json}'"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
